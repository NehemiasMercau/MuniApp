            <th>
                @Html.DisplayNameFor(model => model.MontoNeto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CobroDetalle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstadoPago)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PagoDetalle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PagoTipo)
