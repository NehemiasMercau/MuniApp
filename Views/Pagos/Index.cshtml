                @Html.DisplayFor(modelItem => item.Monto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MontoNeto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CobroDetalle.CobroDetalleId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoPago.EstadoPagoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PagoDetalle.PagoDetalleId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PagoTipo.PagoTipoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PersonaEntidadDeutaDetalle.PersonaEntidadDeudaDetalleId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PagoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PagoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PagoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
