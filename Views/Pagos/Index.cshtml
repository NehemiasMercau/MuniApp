            <th>
                @Html.DisplayNameFor(model => model.PersonaEntidadDeutaDetalle)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Referencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PagoReferencia)
            </td>
