@model IEnumerable<MuniApp.Negocio.entidades.DeudaDetalle>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Importe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Recargo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaVencimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Activo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Deuda)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Periodo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Importe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Recargo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaVencimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Activo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Deuda.DeudaId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Periodo.PeriodoId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DeudaDetalleId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DeudaDetalleId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DeudaDetalleId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
